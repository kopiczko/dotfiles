{
    "title": "GoTo App",
    "rules": [
        {
            "description": "GoTo Google Chrome",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "q",
                        "modifiers": {
                            "mandatory": [
                                "left_control",
                                "left_option",
                                "left_shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Google Chrome.app'"
                        }
                    ]
                }
            ]
        },
        {
            "description": "GoTo iTerm",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "mandatory": [
                                "left_control",
                                "left_option",
                                "left_shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'iTerm.app'"
                        }
                    ]
                }
            ]
        }
    ]
}

        "version": "0.2.0",
        "configurations": [
                {
                        "name": "Test function",
                        "type": "go",
                        "request": "launch",
                        "mode": "test",
                        "program": "./api/types/accesslist/convert/v1",
                        "args": [
                                "-test.run",
                                "TestConvAccessList/audit_with_only_Recurrence.DayOfMonth_set",
                         ],
                        "showLog": true,
                },
                {
                        "name": "Run ./e/tool/teleport",
                        "type": "go",
                        "request": "launch",
                        "mode": "debug",
                        "program": "e/tool/teleport",
                        "args": [
                                "start",
                                "--config",
                                "/Users/kopiczko/.teleport/local1/teleport.yaml",
                        ],
                        "cwd": "${workspaceFolder}",
                        // NOTE: check output of `make -C ./e -j build/teleport`
                        "buildFlags": [
                                "-buildvcs=false",
                                 "-tags", "webassets_embed webassets_ent desktop_access_rdp kustomize_disable_go_plugin_support",
                                 //"-ldflags", "-w -s",
                                 //"-trimpath",
                                 //"-buildmode=pie",
                        ],
                        "env": {
                                "CGO_ENABLED": "1",
                                //"CGO_CFLAGS": "-mmacosx-version-min=11.0",
                                "TELEPORT_UNSTABLE_SKIP_VERSION_UPGRADE_CHECK": "yes",
                        },
                },
                // make TELEPORT_DEBUG=true -C ./e -j build/teleport && e/build/teleport start --config ../teleport/e/pawel/teleport.yaml
                {
                        "name": "Attach ./e/tool/teleport",
                        "type": "go",
                        "request": "attach",
                        "mode": "local",
                        "cwd": "${workspaceFolder}",
                },
                // Terraform plugin debug
                {
                        "name": "Terraform provider",
                        "type": "go",
                        "request": "launch",
                        "mode": "debug",
                        "program": "integrations/terraform/main.go",
                        "internalConsoleOptions": "openOnSessionStart",
                        "args": [
                                "-debug",
                        ],
                        "cwd": "${workspaceFolder}",
                        // ./e/build/tctl --config ~/.teleport/local1/teleport.yaml terraform env | sed -e 's/^export //' -e '/^#/d' >/tmp/tf.env
                        "envFile": "${workspaceFolder}/pawel.d/env"
                },
        ]
}

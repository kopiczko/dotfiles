[user]
  name = Pawel Kopiczko
  email = kopiczko@gmail.com
[core]
  excludesfile = /Users/pawel.kopiczko/.gitignore_global
[alias]
  c = commit
  ca = commit --amend 	
  cleanup = !git remote prune origin && git gc && git clean -df && git stash clear
  co = checkout
  d = diff
  dc = diff --cached
  dw = diff -w
  dcw = diff --cached -w
  dwc = diff --cached -w
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  s = status -uall
[push]
  default = current
[color]
  diff = auto
  status = auto
  branch = auto
[github]
  user = kopiczko
[init]
  templatedir = ~/.git_template
